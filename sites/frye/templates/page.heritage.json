        "template--17038883914005__28bd5bd3-f0a6-448b-afb0-7433b5c28946-166969799436d4c761-0": {
          "type": "heading",
          "settings": {
            "heading": "THE ORIGINAL. THE AUTHENTIC. THE ONLY.",
            "heading_size": "frye-headline-2",
            "heading_color": "#ffffff",
