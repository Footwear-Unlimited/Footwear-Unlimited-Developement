        "6e9f9567-946c-4a09-8fff-be803cfe4694": {
          "type": "heading",
          "settings": {
            "text": "FROM STEAM TO ELECTRICITY",
            "text_color": "#000000",
            "text_size": "frye-headline-3"
          }
        },
        "3fa97b00-6e3a-4169-b5d8-81db34c1351e": {
          "type": "description",
          "settings": {
            "text": "<p>The Frye factory was the first of the nearly twenty firms in Marlboro to convert from steam to electrical power for its operation. Making Frye the first factory in town to run on electricity.<\/p>",
            "text_style": "frye-body",
            "text_color": "#000000"
          }
        }
      },
