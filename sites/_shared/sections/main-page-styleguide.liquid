      <h2>Inputs</h2>
      <div class="inputs">
          <div style="display:flex; flex-wrap: wrap; gap: 3rem; margin-bottom: 2rem;">
              <div>
                  <p>Input Field - Blank</p>
                  <div class="name-form__field-wrapper" style="margin-bottom: 2rem;">
                      <div class="field">
                          <input class="field__input input--large" value="" aria-required="true" autocorrect="off" autocapitalize="none" placeholder="Placeholder goes here" required="" type="text" /><span> </span>
                          <label class="field__label input--large" for="name-form">Placeholder label</label>
                      </div>
                  </div>
              </div>

              <div>
                  <p>Input Field - Filled</p>
                  <div class="name-form__field-wrapper" style="margin-bottom: 2rem;">
                      <div class="field">
                          <input class="field__input input--large" value="Input content here" aria-required="true" autocorrect="off" autocapitalize="none" placeholder="Placeholder goes here" required="" type="text" /><span> </span>
                          <label class="field__label input--large" for="name-form">Placeholder label</label>
                      </div>
                  </div>

              </div>             
          </div>

  
          <div style="display:flex; flex-wrap: wrap; gap: 3rem; margin-bottom: 2rem;">
              <div style="width: 250px">
                  <p>Dropdown Blank</p>
                  <div style="margin-bottom: 2rem;">
                      <div class="name__input--dropdown">
                          <label class="form__label" for="Option-">Select label</label>
                          <div class="select">
                              <span> </span>
                              <select class="select__select input--large" name="options[option]" form="form-name">
                                  <option value="" disabled selected>Select your option</option>
                                  <option value="value-1">Value 1</option>
                                  <option value="value-2">Value 2</option>
                                  <option value="value-3">Value 3</option>
                              </select>
                              <span> </span>
                              <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-caret" viewbox="0 0 10 6">
                                  <path fill-rule="evenodd" clip-rule="evenodd" d="M9.354.646a.5.5 0 00-.708 0L5 4.293 1.354.646a.5.5 0 00-.708.708l4 4a.5.5 0 00.708 0l4-4a.5.5 0 000-.708z" fill="currentColor"></path>
                              </svg>
                          </div>
                      </div>
                  </div>

              </div>

              <div style="width: 250px">
                  <p>Dropdown Selected</p>
                  <div style="margin-bottom: 2rem;">
                      <div class="name__input--dropdown">
                          <label class="form__label" for="Option-">Select label</label>
                          <div class="select">
                              <span> </span>
                              <select class="select__select input--large" name="options[option]" form="form-name">
                                  <option value="value-1">Value 1</option>
                                  <option value="value-2">Value 2</option>
                                  <option value="value-3">Value 3</option>
                              </select>
                              <span> </span>
                              <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-caret" viewbox="0 0 10 6">
                                  <path fill-rule="evenodd" clip-rule="evenodd" d="M9.354.646a.5.5 0 00-.708 0L5 4.293 1.354.646a.5.5 0 00-.708.708l4 4a.5.5 0 00.708 0l4-4a.5.5 0 000-.708z" fill="currentColor"></path>
                              </svg>
                          </div>
                      </div>
                  </div>
              </div>

              </div>
          </div>

          <h3>Radio Selectors</h3>

          <p class="inputs-set">
              <input type="radio" checked="checked" name="test-input" /><span> </span><input type="radio" name="test-input" />
          </p>

          <h3>Checkbox Selectors</h3>

          <p class="inputs-set">
              <input type="checkbox" checked="checked" /><span> </span><input type="checkbox" />
          </p>

      </div>
  </div>

</div>

{% schema %}
{
"name": "Styleguide",
